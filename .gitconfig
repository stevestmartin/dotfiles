[user]
	name = Stephen St. Martin
	email = steve@stevestmartin.com
[color]
	branch = auto
	diff = auto
  grep = auto
	interactive = auto
  showbranch = auto
	status = auto
	ui = 1
[core]
	editor = /usr/local/bin/vim
	excludesfile = ~/.gitignore
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
